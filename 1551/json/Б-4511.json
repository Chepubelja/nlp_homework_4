  \u042f \u0437\u0432\u0435\u0440\u0442\u0430\u0432\u0441\u044f \u0437 \u0446\u044c\u043e\u0433\u043e \u043f\u0438\u0442\u0430\u043d\u043d\u044f \u0434\u043e \u043f\u0435\u043d\u0441\u0456\u0439\u043d\u043e\u0433\u043e \u0444\u043e\u043d\u0434\u0443. \u041c\u0435\u043d\u0456 \u043e\u0444\u0456\u0446\u0456\u0439\u043d\u043e \u043f\u043e\u0432\u0456\u0434\u043e\u043c\u0438\u043b\u0438, \u0449\u043e \u044f \u0436\u043e\u0434\u043d\u0438\u0445 \u043f\u0456\u043b\u044c\u0433 \u043d\u0435 \u043c\u0430\u044e \u0456 \u0442\u043e\u043c\u0443 \u043f\u0435\u0440\u0435\u043d\u043e\u0441\u0438\u0442\u0438 \u0442\u0435\u0440\u043c\u0456\u043d \u0432\u0438\u043f\u043b\u0430\u0442\u0438 \u043f\u0435\u043d\u0441\u0456\u0457 \u0437 25 \u0447\u0438\u0441\u043b\u0430, \u0441\u043a\u0430\u0436\u0456\u043c\u043e \u043d\u0430 19-20 \u0447\u0438\u0441\u043b\u043e \u0432\u043e\u043d\u0438 \u0432\u0456\u0434\u043c\u043e\u0432\u0438\u043b\u0438\u0441\u044c. \u0412\u043e\u043d\u0438 \u043d\u0430\u0434\u0430\u043b\u0438 \u043d\u0430\u0441\u0442\u0443\u043f\u043d\u0435 \u043f\u043e\u044f\u0441\u043d\u0435\u043d\u043d\u044f:  \u0429\u043e \u044f \u043d\u0456\u0447\u0438\u043c \u043d\u0435 \u043a\u0440\u0430\u0449\u0435 \u0456\u043d\u0448\u0438\u0445 \u043f\u0435\u043d\u0441\u0456\u043e\u043d\u0435\u0440\u0456\u0432 \u0456, \u044f\u043a\u0449\u043e \u043c\u0435\u043d\u0456 \u043f\u0435\u0440\u0435\u043d\u0435\u0441\u0443\u0442\u044c \u0434\u0430\u0442\u0443, \u0442\u043e \u0432\u043e\u043d\u0438 \u0431\u0443\u0434\u0443\u0442\u044c \u0437\u043c\u0443\u0448\u0435\u043d\u0456 \u044f\u043a\u043e\u0433\u043e\u0441\u044c \u0456\u043d\u0448\u043e\u0433\u043e \u043f\u0435\u043d\u0441\u0456\u043e\u043d\u0435\u0440\u0430 \u043f\u0435\u0440\u0435\u0432\u0435\u0441\u0442\u0438 \u043d\u0430 25 \u0447\u0438\u0441\u043b\u043e. \u0406 \u0432 \u0456\u043d\u0448\u043e\u0433\u043e \u043f\u0435\u043d\u0441\u0456\u043e\u043d\u0435\u0440\u0430 \u043f\u043e\u0447\u043d\u0443\u0442\u044c\u0441\u044f \u0442\u0430\u043a\u0456 \u0436 \u0441\u0430\u043c\u0456 \u043f\u0440\u043e\u0431\u043b\u0435\u043c\u0438.  \u0427\u0438\u043c \u044f \u043a\u0440\u0430\u0449\u0435 \u0456\u043d\u0448\u0438\u0445 \u043f\u0435\u043d\u0441\u0456\u043e\u043d\u0435\u0440\u0456\u0432? \u0412 \u043f\u0440\u0438\u043d\u0446\u0438\u043f\u0456 \u044f \u0437\u0433\u0456\u0434\u043d\u0438\u0439 \u0437 \u0457\u0445 \u043f\u043e\u044f\u0441\u043d\u0435\u043d\u043d\u044f\u043c. \u0423 \u043d\u0438\u0445 \u0454 \u0441\u0432\u0456\u0439 \u0433\u0440\u0430\u0444\u0456\u043a \u0432\u0438\u043f\u043b\u0430\u0442\u0438 \u043f\u0435\u043d\u0441\u0456\u0439 \u0456 \u0432\u0441\u0456 \u043f\u0435\u043d\u0441\u0456\u043e\u043d\u0435\u0440\u0438 \u0440\u043e\u0437\u043f\u043e\u0434\u0456\u043b\u0435\u043d\u0456 \u0440\u0456\u0432\u043d\u043e\u043c\u0456\u0440\u043d\u043e \u043f\u043e \u0434\u043d\u044f\u0445 \u0432\u0438\u043f\u043b\u0430\u0442 \u043f\u0435\u043d\u0441\u0456\u0439.\r\n   \u0422\u0430\u043a\u043e\u0436 \u0412\u0438 \u0440\u0435\u043a\u043e\u043c\u0435\u043d\u0434\u0443\u0454\u0442\u0435 \u0437\u0432\u0435\u0440\u0442\u0430\u0442\u0438\u0441\u044c \u0434\u043e: \u201d...\u0420\u0430\u0439\u043e\u043d\u043d\u0438\u0445 \u0432 \u043c\u0456\u0441\u0442\u0456 \u041a\u0438\u0454\u0432\u0456 \u0434\u0435\u0440\u0436\u0430\u0432\u043d\u0438\u0445 \u0430\u0434\u043c\u0456\u043d\u0456\u0441\u0442\u0440\u0430\u0446\u0456\u044f\u0445 \u0443\u0442\u0432\u043e\u0440\u0435\u043d\u043e \u0441\u043f\u0435\u0446\u0456\u0430\u043b\u044c\u043d\u0443 \u043a\u043e\u043c\u0456\u0441\u0456\u044e, \u0437\u0432\u0435\u0440\u043d\u0443\u0432\u0448\u0438\u0441\u044c \u0434\u043e \u044f\u043a\u043e\u0457 \u0432\u0438 \u043c\u0430\u0454\u0442\u0435 \u043c\u043e\u0436\u043b\u0438\u0432\u0456\u0441\u0442\u044c \u0432\u0438\u0440\u0456\u0448\u0438\u0442\u0438 \u043f\u043e\u0440\u0443\u0448\u0435\u043d\u0435 \u0432\u0430\u043c\u0438 \u043f\u0438\u0442\u0430\u043d\u043d\u044f\u201d, \u043d\u0430 \u0449\u043e \u0432\u0456\u0434\u043f\u043e\u0432\u0456\u0434\u0430\u044e, \u0449\u043e \u0432\u0441\u0456 \u043c\u043e\u0457 \u043f\u043e\u043f\u0435\u0440\u0435\u0434\u043d\u0456 \u0437\u0430\u044f\u0432\u0438 \u0412\u0430\u043c\u0438 \u0436 \u043f\u0435\u0440\u0435\u043d\u0430\u043f\u0440\u0430\u0432\u043b\u044f\u043b\u0438\u0441\u044f \u0434\u043e \u0440\u0430\u0439\u043e\u043d\u043d\u043e\u0457 (\u0421\u0432\u044f\u0442\u043e\u0448\u0438\u043d\u0441\u044c\u043a\u043e\u0457) \u0434\u0435\u0440\u0436\u0430\u0434\u043c\u0456\u043d\u0456\u0441\u0442\u0440\u0430\u0446\u0456\u0457. \u0412\u0438 \u043c\u043e\u0436\u0435\u0442\u0435 \u043f\u0435\u0440\u0435\u0432\u0456\u0440\u0438\u0442\u0438 \u0437\u043c\u0456\u0441\u0442 \u043c\u043e\u0457\u0445 \u0437\u0430\u044f\u0432 \u0411-3370, \u0411-2815, \u041127706, \u0442\u0430 \u0432\u0456\u0434\u043f\u043e\u0432\u0456\u0434\u0456 \u043d\u0430 \u043d\u0438\u0445 \u0456 \u043f\u043e\u0431\u0430\u0447\u0438\u0442\u0438, \u0449\u043e \u0436\u043e\u0434\u043d\u0430 \u043c\u043e\u044f \u0437\u0430\u044f\u0432\u0430 \u0440\u0430\u0439\u043e\u043d\u043d\u043e\u044e \u0434\u0435\u0440\u0436\u0430\u0434\u043c\u0456\u043d\u0456\u0441\u0442\u0440\u0430\u0446\u0456\u0454\u044e \u043f\u043e \u0441\u0443\u0442\u0456, \u0442\u0430 \u043d\u0430 \u043a\u043e\u043c\u0456\u0441\u0456\u0457, \u043d\u0435 \u0440\u043e\u0437\u0433\u043b\u044f\u0434\u0430\u043b\u0430\u0441\u044c \u0456 \u044f \u043e\u0442\u0440\u0438\u043c\u0443\u0432\u0430\u0432 \u0442\u0456\u043b\u044c\u043a\u0438 \u0432\u0456\u0434\u043f\u0438\u0441\u043a\u0438, \u0449\u043e \u0456 \u0454 \u043f\u0440\u0438\u0447\u0438\u043d\u043e\u044e \u0447\u0435\u0440\u0433\u043e\u0432\u043e\u0457 \u0441\u043a\u0430\u0440\u0433\u0438. \u041e\u0441\u043a\u0456\u043b\u044c\u043a\u0438 \u0434\u0435-\u0444\u0430\u043a\u0442\u043e \u201d\u0443\u0442\u0432\u043e\u0440\u0435\u043d\u0456 \u0441\u043f\u0435\u0446\u0456\u0430\u043b\u044c\u043d\u043e \u043a\u043e\u043c\u0456\u0441\u0456\u0457\u201d \u043c\u043e\u0454 \u043f\u0438\u0442\u0430\u043d\u043d\u044f \u043d\u0435 \u0440\u043e\u0437\u0433\u043b\u044f\u0434\u0430\u043b\u0438.\r\n   \u0417\u0430\u043c\u0456\u0441\u0442\u044c \u0442\u043e\u0433\u043e, \u0449\u043e\u0431 \u043f\u0438\u0441\u0430\u0442\u0438 \u043c\u0435\u043d\u0456 \u0432\u0456\u0434\u043f\u0438\u0441\u043a\u0438, \u0432\u043e\u043d\u0438 \u043c\u043e\u0433\u043b\u0438 \u0431 \u0437\u0432\u0435\u0440\u043d\u0443\u0442\u0438\u0441\u044c \u0434\u043e \u043e\u0431\u0447\u0438\u0441\u043b\u044e\u0432\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u0446\u0435\u043d\u0442\u0440\u0443, \u044f\u043a\u0438\u0439 \u0434\u0440\u0443\u043a\u0443\u0454 \u0436\u0438\u0442\u043b\u043e\u0432\u043e-\u043a\u043e\u043c\u0443\u043d\u0430\u043b\u044c\u043d\u0456 \u043f\u043b\u0430\u0442\u0456\u0436\u043a\u0438, \u0449\u043e\u0431 \u0432\u043e\u043d\u0438 \u0432 \u043a\u043e\u043c\u043f\u2019\u044e\u0442\u0435\u0440\u0456 \u043f\u0435\u0440\u0435\u043d\u0435\u0441\u043b\u0438 \u043a\u0456\u043d\u0446\u0435\u0432\u0438\u0439 \u0442\u0435\u0440\u043c\u0456\u043d \u0441\u043f\u043b\u0430\u0442\u0438 \u0437 20 \u0447\u0438\u0441\u043b\u0430, \u0441\u043a\u0430\u0436\u0456\u043c\u043e \u043d\u0430 26 \u0447\u0438\u0441\u043b\u043e \u0456 \u043f\u0438\u0442\u0430\u043d\u043d\u044f \u0431\u0443\u043b\u043e \u0431 \u0432\u0438\u0440\u0456\u0448\u0435\u043d\u043e. \u0406 \u043f\u0456\u0434\u0432\u0438\u0449\u0435\u043d\u0456 \u0442\u0430\u0440\u0438\u0444\u0438 \u0431\u0438 \u0430\u0432\u0442\u043e\u043c\u0430\u0442\u0438\u0447\u043d\u043e \u043d\u0435 \u043d\u0430\u0440\u0430\u0445\u043e\u0432\u0443\u0432\u0430\u043b\u0438\u0441\u044f.\r\n   \u041e\u0441\u044c \u0412\u0438 \u0456 \u043c\u0430\u0454\u0442\u0435 \u043c\u043e\u0436\u043b\u0438\u0432\u0456\u0441\u0442\u044c \u0437\u0430\u043f\u0438\u0442\u0430\u0442\u0438 \u0443 \u0421\u0432\u044f\u0442\u043e\u0448\u0438\u043d\u0441\u044c\u043a\u0456\u0439 \u0440\u0430\u0439\u0434\u0435\u0440\u0436\u0430\u0434\u043c\u0456\u043d\u0456\u0441\u0442\u0440\u0430\u0446\u0456\u0457,