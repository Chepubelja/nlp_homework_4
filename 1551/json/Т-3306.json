u0434\u0443\u0442\u0441\u044f \u0432 \u043e\u0441\u043d\u043e\u0432\u043d\u043e\u043c \u043d\u0430 \u041f\u0430\u0441\u0445\u0443, \u0437\u0430\u0442\u043e \u0435\u0441\u0442\u044c \u043f\u043b\u0430\u0442\u043d\u0430\u044f \u0441\u0442\u043e\u044f\u043d\u043a\u0430, \u0432\u043e\u043a\u0440\u0443\u0433 \u0432\u0441\u0435\u0439 \u0446\u0435\u0440\u043a\u0432\u0438, \u043d\u0430 \u043a\u043e\u0442\u043e\u0440\u043e\u0439 \u0438\u0441\u043f\u0440\u0430\u0432\u043d\u043e \u0431\u0435\u0440\u0443\u0442 20 \u0433\u0440\u043d \u0438 \u043d\u0435 \u0432\u044b\u0434\u0430\u044e\u0442 \u043d\u0438\u043a\u0430\u043a\u0438\u0445 \u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442\u043e\u0432. 25 \u043b\u0435\u0442 \u0443\u0447\u0430\u0441\u0442\u043e\u043a \u043e\u0431\u0435\u043d\u0441\u0451\u043d \u0441\u0442\u0440\u043e\u0438\u0442\u0435\u043b\u044c\u043d\u044b\u043c \u0437\u0430\u0431\u043e\u0440\u043e\u043c \u0438 \u043d\u0430 \u043d\u0435\u043c \u043d\u0438\u0447\u0435\u0433\u043e, \u043a\u0440\u043e\u043c\u0435 \u0441\u0442\u043e\u044f\u043d\u043a\u0438 \u0430\u0432\u0442\u043e \u0438 \u043f\u0430\u0441\u0445\u0430\u043b\u044c\u043d\u044b\u0445 \u0441\u043b\u0443\u0436\u0431, \u043d\u0435 \u043f\u0440\u043e\u0438\u0441\u0445\u043e\u0434\u0438\u0442!\r\n\r\n\u0425\u043e\u0447\u0443 \u043f\u043e\u043b\u0443\u0447\u0438\u0442\u044c \u0441\u043b\u0435\u0434\u0443\u044e\u0449\u0443\u044e \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u044e:\r\n1. \u041f\u0440\u043e\u0432\u043e\u0434\u0438\u043b\u0438\u0441\u044c \u043b\u0438 \u201d\u0433\u0440\u043e\u043c\u0430\u0434\u0441\u044c\u043a\u0456 \u0441\u043b\u0443\u0445\u0430\u043d\u043d\u044f\u201d \u0441\u0440\u0435\u0434\u0438 \u0436\u0438\u0442\u0435\u043b\u0435\u0439 \u0434\u0430\u043d\u043d\u043e\u0433\u043e \u043c\u0438\u043a\u0440\u043e\u0440\u0430\u0439\u043e\u043d\u0430, \u043f\u0435\u0440\u0435\u0434 \u0432\u044b\u0434\u0435\u043b\u0435\u043d\u0438\u0435\u043c \u0437\u0435\u043c\u043b\u0438 \u0434\u043b\u044f \u0434\u0430\u043d\u043d\u043e\u0439 \u0446\u0435\u0440\u043a\u0432\u0438, \u0435\u0441\u043b\u0438 \u0434\u0430, \u0442\u043e \u043a\u043e\u0433\u0434\u0430 \u043a\u0435\u043c \u0438 \u0433\u0434\u0435?\r\n2. \u041a\u0442\u043e \u0434\u0430\u043b \u0440\u0430\u0437\u0440\u0435\u0448\u0435\u043d\u0438\u0435, \u0443\u043d\u0438\u0447\u0442\u043e\u0436\u0438\u0442\u044c \u0441\u043a\u0432\u0435\u0440, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u0442\u0430\u043c \u0431\u044b\u043b \u0438 \u043f\u0435\u0440\u0435\u0433\u043e\u0440\u043e\u0434\u0438\u0442\u044c \u0432\u0435\u0441\u044c \u043a\u0432\u0430\u0440\u0442\u0430\u043b, \u0442\u0430\u043a\u0438\u043c \u043e\u0431\u0440\u0430\u0437\u043e\u043c, \u0447\u0442\u043e \u0431\u044b \u0436\u0438\u0442\u0435\u043b\u0438 \u043d\u0435 \u043c\u043e\u0433\u043b\u0438 \u0447\u0435\u0440\u0435\u0437 \u043d\u0435\u0433\u043e \u043f\u0440\u043e\u0439\u0442\u0438? \u041d\u0430\u043f\u0440\u0438\u043c\u0435\u0440 \u043e\u0442 9\u0433\u043e \u0434\u043e\u043c\u0430 \u043d\u0430 \u0414\u0430\u0440\u043d\u0438\u0446\u043a\u043e\u043c \u0431\u0443\u043b\u044c\u0432\u0430\u0440\u0435 \u0431\u044b\u043b\u0430 \u0434\u043e\u0440\u0436\u043a\u0430 \u043d\u0430 \u0443\u043b. \u0411\u043e\u0439\u0447\u0435\u043d\u043a\u043e. \u0424\u0418\u041e \u0447\u0438\u043d\u043e\u0432\u043d\u0438\u043a\u0430, \u043d\u0430\u0437\u0432\u0430\u043d\u0438\u0435 \u043e\u0440\u0433\u0430\u043d\u0430 \u0438 \u043d\u043e\u043c\u0435\u0440 \u0440\u0430\u0441\u043f\u043e\u0440\u044f\u0436\u0435\u043d\u0438\u044f, \u043a\u0442\u043e \u0434\u0430\u043b \u0440\u0430\u0437\u0440\u0435\u0448\u0435\u043d\u0438\u0435 \u043d\u0430 \u044d\u0442\u043e \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u0435.\r\n3. \u041a\u0442\u043e \u0434\u0430\u043b \u0440\u0430\u0437\u0440\u0435\u0448\u0435\u043d\u0438\u0435 \u043d\u0430 \u0432\u044b\u0440\u0443\u0431\u043a\u0443 \u0432\u0441\u0435\u0433\u043e \u0441\u043a\u0432\u0435\u0440\u0430, \u0442\u0430\u043c \u0431\u044b\u043b\u043e \u043e\u043a\u043e\u043b\u043e 100 \u043a\u043b\u0451\u043d\u043e\u0432, \u043f\u043e\u0441\u0430\u0436\u0435\u043d\u044b\u0445 \u0432 70\u0435 \u0433\u043e\u0434\u044b? \u0424\u0418\u041e \u0447\u0438\u043d\u043e\u0432\u043d\u0438\u043a\u0430, \u043d\u0430\u0437\u0432\u0430\u043d\u0438\u0435 \u043e\u0440\u0433\u0430\u043d\u0430 \u0438 \u043d\u043e\u043c\u0435\u0440 \u0440\u0430\u0441\u043f\u043e\u0440\u044f\u0436\u0435\u043d\u0438\u044f\r\n4. \u041a\u0442\u043e \u0434\u0430\u043b \u0440\u0430\u0437\u0440\u0435\u0448\u0435\u043d\u0438\u0435 \u043d\u0430 \u0441\u043e\u043e\u0440\u0443\u0436\u0435\u043d\u0438\u0435 \u0441\u0442\u043e\u044f\u043d\u043a\u0438? (\u0424\u0418\u041e \u0447\u0438\u043d\u043e\u0432\u043d\u0438\u043a\u043e\u0432 \u0438 \u043d\u0430\u0437\u0432\u0430\u043d\u0438\u0435 \u043e\u0440\u0433\u0430\u043d\u0430, \u0442\u0430\u043a \u0436\u0435 \u043d\u043e\u043c\u0435\u0440 \u0440\u0430\u0441\u043f\u043e\u0440\u044f\u0436\u0435\u043d\u0438\u044f \u041a\u041c\u0414\u0410 \u0438 \u0435\u0433\u043e \u0434\u0430\u0442\u0430) \u0415\u0441\u043b\u0438 \u0441\u0442\u043e\u044f\u043d\u043a\u0430 \u043d\u0435 \u0437\u0430\u043a\u043e\u043d\u043d\u0430, \u0442\u043e \u0442\u0430 \u0436\u0435 \u0441\u0430\u043c\u0430\u044f \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u044f \u043e \u0446\u0435\u0440\u043a\u0432\u0438, \u0441 \u0446\u0435\u043b\u0435\u0432\u044b\u043c \u043d\u0430\u0437\u043d\u0430\u0447\u0435\u043d\u0438\u0435\u043c \u0443\u0447\u0430\u0441\u0442\u043a\u0430, \u0438\u043d\u0432\u0435\u0441\u0442\u0438\u0446\u0438\u043e\u043d\u043d\u044b\u043c\u0438 \u043e\u0431\u044f\u0437\u0430\u0442\u0435\u043b\u044c\u0441\u0442\u0432\u0430\u043c\u0438 (\u043a\u0430\u043a \u0431\u044b\u0441\u0442\u0440\u043e \u0434\u043e\u043b\u0436\u043d\u044b \u0431\u044b\u043b\u0438 \u0431\u044b\u0442\u044c \u0432\u043e\u0437\u0432\u0435\u0434\u0435\u043d\u044b \u0437\u0434\u0430\u043d\u0438\u044f \u0438 \u043e\u0431\u043b\u0430\u0433\u043e\u0440\u043e\u0436\u0435\u043d\u0430 \u0442\u0435\u0440\u0440\u0438\u0442\u043e\u0440\u0438\u044f), \u0430 \u0442\u0430\u043a \u0436\u0435 \u0440\u0430\u0437\u043c\u0435\u0440\u0430\u043c\u0438 \u0438 \u0433\u0440\u0430\u043d\u0438\u0446\u0430\u043c\u0438 \u0443\u0447\u0430\u0441\u0442\u043a\u0430.\r\n5. \u041f\u043b\u0430\u0442\u0438\u0442 \u043b\u0438 \u0441\u0442\u043e\u044f\u043d\u043a\u0430 \u043d\u0430\u043b\u043e\u0433\u0438?\r\n6. \u041a\u0430\u043a \u043e\u0444\u043e\u0440\u043c\u043b\u0435\u043d\u044b \u0440\u0430\u0431\u043e\u0442\u043d\u0438\u043a\u0438 \u0441\u0442\u043e\u044f\u043d\u043a\u0438, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0432\u0437\u044b\u043c\u0430\u044e\u0442 \u043f\u043b\u0430\u0442\u0443 \u0437\u0430 \u0432\u044a\u0435\u0437\u0434?\r\n7. \u041a\u0430\u043a\u0438\u043c \u043e\u0431\u0440\u0430\u0437\u043e\u043c \u043a\u043e\u043c\u043c\u0435\u0440\u0447\u0435\u0441\u043a\u0430\u044f \u0441\u0442\u043e\u044f\u043d\u043a\u0430 \u043f\u043e\u044f\u0432\u0438\u043b\u0430\u0441\u044c \u043d\u0430 \u0442\u0435\u0440\u0440\u0438\u0442\u043e\u0440\u0438\u0438 \u0446\u0435\u0440\u043a\u0432\u0438?\r\n8. \u041a\u043e\u0433\u0434\u0430 \u0431\u0443\u0434\u0435\u0442 \u043d\u0430\u0432\u0435\u0434\u0451\u043d \u043f\u043e\u0440\u044f\u0434\u043e\u043a \u043d\u0430 \u0434\u0430\u043d\u043d\u043e\u043c \u0443\u0447\u0430\u0441\u0442\u043a\u0435 \u0438 \u043a\u0442\u043e \u043e\u0442\u0432\u0435\u0447\u0430\u0435\u0442 \u0437\u0430 \u044d\u0442\u043e\u0442 \u043f\u0440\u043e\u0446\u0435\u0441\u0441?\r\n\r\n\u0417\u0430\u0440\u0430\u043d\u0435\u0435 \u0431\u043b\u0430\u0433\u043e\u0434\u0430\u0440\u0435\u043d \u0437\u0430 \u0431\u044b\u0441\u0442\u0440\u044b\u0439 \u043e\u0442\u0432\u0435\u0442 \u043d\u0430 \u0412\u0421\u0415 \u043f\u043e\u0441\u0442\u0430\u0432\u043b\u0435\u043d\u043d\u044b\u0435 \u0432 \u044d\u0442\u043e\u043c \u043e\u0431\u0440\u0430\u0449\u0435\u043d\u0438\u0438 \u0432\u043e\u043f\u0440\u043e\u0441\u044b, \u0432 \u0443\u0441\u0442\u0430\u043d\u043e\u0432