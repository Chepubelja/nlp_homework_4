0441\u0442\u0438 \u0437\u0430 \u043f\u043e\u0441\u0442\u0430\u0432\u043a\u0443 \u0433\u043e\u0440\u044f\u0447\u0435\u0439 \u0432\u043e\u0434\u044b \u043e\u0442 \u041a\u0438\u0435\u0432\u044d\u043d\u0435\u0440\u0433\u043e), \u0432 \u043a\u043e\u0442\u043e\u0440\u044b\u0445 \u0443\u043a\u0430\u0437\u0430\u043d\u043e, \u0447\u0442\u043e \u0441 \u044f\u043d\u0432\u0430\u0440\u044f 2015 \u0433\u043e\u0434\u0430 \u0432 \u043a\u0432\u0430\u0440\u0442\u0438\u0440\u0435 \u043f\u043e \u0430\u0434\u0440\u0435\u0441\u0443 \u043c. \u041a\u0438\u0457\u0432, \u043f\u0440. \u0414\u0435\u043c\u0456\u0457\u0432\u0441\u044c\u043a\u0438\u0439 6, \u043a\u0432. 12 \u0437\u0430\u0440\u0435\u0433\u0438\u0441\u0442\u0440\u0438\u0440\u043e\u0432\u0430\u043d \u00ab1\u00bb \u0447\u0435\u043b\u043e\u0432\u0435\u043a, \u044f \u043f\u043e\u0434\u0443\u043c\u0430\u043b, \u0447\u0442\u043e \u043f\u0440\u043e\u0441\u0442\u043e \u0440\u0430\u0431\u043e\u0442\u043d\u0438\u043a\u0438 \u041a\u0438\u0435\u0432\u044d\u043d\u0435\u0440\u0433\u043e \u043e\u0448\u0438\u0431\u043b\u0438\u0441\u044c, \u0442\u0430\u043a \u043a\u0430\u043a \u0434\u043e \u044d\u0442\u043e\u0433\u043e \u0432 \u043a\u0432\u0438\u0442\u0430\u043d\u0446\u0438\u044f\u0445 \u0431\u044b\u043b\u043e \u0432\u0435\u0440\u043d\u043e \u0443\u043a\u0430\u0437\u0430\u043d\u043e \u00ab0\u00bb \u0437\u0430\u0440\u0435\u0433\u0438\u0441\u0442\u0440\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u044b\u0445 \u043b\u0438\u0446 (\u0432 \u0443\u043a\u0430\u0437\u0430\u043d\u043d\u043e\u0439 \u043a\u0432\u0430\u0440\u0442\u0438\u0440\u0435 \u043d\u0435 \u0437\u0430\u0440\u0435\u0433\u0438\u0441\u0442\u0440\u0438\u0440\u043e\u0432\u0430\u043d \u0438 \u0444\u0430\u043a\u0442\u0438\u0447\u0435\u0441\u043a\u0438 \u043d\u0435 \u043f\u0440\u043e\u0436\u0438\u0432\u0430\u0435\u0442 \u043d\u0438 \u043e\u0434\u0438\u043d \u0447\u0435\u043b\u043e\u0432\u0435\u043a \u0438 \u043a \u0442\u043e\u043c\u0443 \u0436\u0435, \u043f\u043e \u043f\u0440\u043e\u0441\u044c\u0431\u0435 \u0440\u0430\u0431\u043e\u0442\u043d\u0438\u043a\u043e\u0432 \u041a\u0438\u0435\u0432\u044d\u043d\u0435\u0440\u0433\u043e \u0432\u0435\u043d\u0442\u0438\u043b\u044c \u0438\u043c\u0438 \u043e\u043f\u043b\u043e\u043c\u0431\u0438\u0440\u043e\u0432\u0430\u043d).\r\n2.\t\u0421\u0432\u044f\u0437\u0430\u0432\u0448\u0438\u0441\u044c \u043f\u043e \u0442\u0435\u043b\u0435\u0444\u043e\u043d\u0443 \u0441 \u041a\u0438\u0435\u0432\u044d\u043d\u0435\u0440\u0433\u043e, \u044f \u0443\u0437\u043d\u0430\u043b, \u0447\u0442\u043e \u0432 \u043a\u0432\u0438\u0442\u0430\u043d\u0446\u0438\u0438 \u0437\u0430 \u044f\u043d\u0432\u0430\u0440\u044c 2015 \u0433\u043e\u0434\u0430 \u0443\u043a\u0430\u0437\u0430\u043d\u043e \u00ab1\u00bb \u0432\u043c\u0435\u0441\u0442\u043e \u00ab0\u00bb \u0437\u0430\u0440\u0435\u0433\u0438\u0441\u0442\u0440\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u044b\u0439 \u0447\u0435\u043b\u043e\u0432\u0435\u043a \u043d\u0430 \u043e\u0441\u043d\u043e\u0432\u0430\u043d\u0438\u0438 \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u0438 \u043f\u043e\u043b\u0443\u0447\u0435\u043d\u043d\u043e\u0439 \u043e\u0442 \u0416\u0415\u041a-103.\r\n3.\t\u041d\u0430 \u0441\u0435\u0433\u043e\u0434\u043d\u044f \u043f\u0440\u043e\u0434\u043e\u043b\u0436\u0430\u044e\u0442 \u043f\u0440\u0438\u0441\u044b\u043b\u0430\u0442\u044c \u043a\u0432\u0438\u0442\u0430\u043d\u0446\u0438\u0438 \u0441 \u0443\u043a\u0430\u0437\u0430\u043d\u043d\u043e\u0439 \u0432 \u043d\u0438\u0445 \u043d\u0435\u0434\u043e\u0441\u0442\u043e\u0432\u0435\u0440\u043d\u043e\u0439 \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u0435\u0439 \u043e \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u0435 \u0437\u0430\u0440\u0435\u0433\u0438\u0441\u0442\u0440\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u044b\u0445 \u0432 \u0443\u043a\u0430\u0437\u0430\u043d\u043d\u043e\u0439 \u043a\u0432\u0430\u0440\u0442\u0438\u0440\u0435 \u043b\u0438\u0446, \u0430 \u0438\u043c\u0435\u043d\u043d\u043e \u00ab1\u00bb \u0432\u043c\u0435\u0441\u0442\u043e \u00ab0\u00bb \u0438  \u041a\u0438\u0435\u0432\u044d\u043d\u0435\u0440\u0433\u043e \u0438 \u041a\u0438\u0435\u0432\u0432\u043e\u0434\u043e\u043a\u0430\u043d\u0430\u043b \u0438 \u041a\u0438\u0435\u0432\u0433\u0430\u0437 \u0438 \u0416\u0415\u041a-103 (\u0444\u043e\u0442\u043e\u043a\u043e\u043f\u0438\u044f \u043f\u0440\u0438\u043b\u0430\u0433\u0430\u0435\u0442\u0441\u044f), \u043d\u0435 \u0441\u043c\u043e\u0442\u0440\u044f \u0434\u0430\u0436\u0435 \u043d\u0430 \u043c\u043e\u0435 \u043e\u0434\u043d\u043e\u0437\u043d\u0430\u0447\u043d\u043e\u0435 \u0438 \u0437\u0430\u043a\u043e\u043d\u043d\u043e\u0435 \u043f\u0438\u0441\u044c\u043c\u0435\u043d\u043d\u043e\u0435 \u0442\u0440\u0435\u0431\u043e\u0432\u0430\u043d\u0438\u0435 \u0432 \u0416\u0415\u041a-103 (\u0444\u043e\u0442\u043e\u043a\u043e\u043f\u0438\u044f \u043f\u0440\u0438\u043b\u0430\u0433\u0430\u0435\u0442\u0441\u044f), \u043d\u0435 \u0442\u0440\u0435\u0431\u0443\u044e\u0449\u0435\u0435 \u043d\u0438 \u0442\u043e\u043b\u043a\u043e\u0432\u0430\u043d\u0438\u0439 \u043d\u0438 \u0440\u0430\u0437\u044a\u044f\u0441\u043d\u0435\u043d\u0438\u0439, \u0430 \u0438\u043c\u0435\u043d\u043d\u043e, \u0443\u043a\u0430\u0437\u044b\u0432\u0430\u0442\u044c \u0432 \u043a\u0432\u0438\u0442\u0430\u043d\u0446\u0438\u044f\u0445 \u0416\u0415\u041a\u043e\u043c \u0438 \u043f\u0435\u0440\u0435\u0434\u0430\u0432\u0430\u0442\u044c \u0416\u0415\u041a\u043e\u043c \u0442\u0440\u0435\u0442\u044c\u0438\u043c \u0437\u0430\u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043e\u0432\u0430\u043d\u043d\u044b\u043c \u043b\u0438\u0446\u0430\u043c (\u0432 \u0447\u0430\u0441\u0442\u043d\u043e\u0441\u0442\u0438, \u041a\u0438\u0435\u0432\u044d\u043d\u0435\u0440\u0433\u043e, \u041a\u0438\u0435\u0432\u0432\u043e\u0434\u043e\u043a\u0430\u043d\u0430\u043b, \u041a\u0438\u0435\u0432\u0433\u0430\u0437) \u0434\u043e\u0441\u0442\u043e\u0432\u0435\u0440\u043d\u0443\u044e \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u044e, \u0430 \u0438\u043c\u0435\u043d\u043d\u043e \u00ab0\u00bb \u0437\u0430\u0440\u0435\u0433\u0438\u0441\u0442\u0440\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u044b\u0445 \u043b\u0438\u0446 \u0432 \u0443\u043a\u0430\u0437\u0430\u043d\u043d\u043e\u0439 \u043a\u0432\u0430\u0440\u0442\u0438\u0440\u0435 \u043b\u0438\u0446, \u0432\u043c\u0435\u0441\u0442\u043e \u0432\u044b\u043c\u044b\u0448\u043b\u0435\u043d\u043d\u043e\u0433\u043e \u0440\u0430\u0431\u043e\u0442\u043d\u0438\u043a\u0430\u043c\u0438 \u0416\u0415\u041a-103 \u00ab1\u00bb (\u0441 \u0442\u0430\u043a\u0438\u043c \u0436\u0435 \u0443\u0441\u043f\u0435\u0445\u043e\u043c \u043e\u043d\u0438 \u043c\u043e\u0433\u0443\u0442 \u0443\u043a\u0430\u0437\u0430\u0442\u044c \u00ab2\u00bb \u0438\u043b\u0438 \u00ab10\u00bb). (\u044d\u0442\u043e \u043f\u0440\u043e\u0434\u043e\u043b\u0436\u0435\u043d\u0438\u0435 - \u043d\u0435 \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e \u043f\u0435\u0440\u0435\u0434\u0430\u0442\u044c \u0442\u0435\u043a\u0441\u0442 \u0446\u0435\u043b\u0438\u043a\u043e\u043c)\r\n\r\n4.\t\u041e\u0442\u0432\u0435\u0442 \u043f\u043e\u043b\u0443\u0447\u0435\u043d\u043d\u044b\u0439 \u043e\u0442 \u0416\u0415\u041a\u0430 (\u0444\u043e\u0442\u043e\u043a\u043e\u043f\u0438\u044f \u043f\u0440\u0438\u043b\u0430\u0433\u0430\u0435\u0442\u0441\u044f) \u0437\u0430 \u043f\u043e\u0434\u043f\u0438\u0441\u044c\u044e \u041a\u0443\u0440\u0431\u0435\u0442 \u0420. \u041c. \u043f\u043e\u0434\u0442\u0432\u0435\u0440\u0436\u0434\u0430\u0435\u0442 \u0441\u043b\u0435\u0434\u0443\u044e\u0449\u0435\u0435:\r\n- \u0416\u0415\u041a \u0443\u043c\u044b\u0448\u043b\u0435\u043d\u043d\u043e \u0443\u043a\u0430\u0437\u044b\u0432\u0430\u0435\u0442 \u0432 \u043a\u0432\u0438\u0442\u0430\u043d\u0446\u0438\u044f\u0445 \u0438 \u043f\u0435\u0440\u0435\u0434\u0430\u0435\u0442 \u0442\u0440\u0435\u0442\u044c\u0438\u043c \u043b\u0438\u0446\u0430\u043c \u0437\u0430\u0432\u0435\u0434\u043e\u043c\u043e \u043d\u0435\u0434\u043e\u0441\u0442\u043e\u0432\u0435\u0440\u043d\u0443\u044e \u0438\u043d\