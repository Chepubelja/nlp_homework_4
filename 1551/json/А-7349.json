3d\u043e\u0432\u043b\u044e\u0432\u0430\u0442\u0438 \u043f\u0440\u0438\u0441\u0442\u0440\u043e\u0457 \u043f\u0440\u0438\u043c\u0443\u0441\u043e\u0432\u043e\u0433\u043e \u0437\u043d\u0438\u0436\u0435\u043d\u043d\u044f \u0448\u0432\u0438\u0434\u043a\u043e\u0441\u0442\u0456 \u0440\u0443\u0445\u0443 \u0442\u0440\u0430\u043d\u0441\u043f\u043e\u0440\u0442\u043d\u0438\u0445 \u0437\u0430\u0441\u043e\u0431\u0456\u0432 \u043d\u0430 \u043c\u0430\u0433\u0456\u0441\u0442\u0440\u0430\u043b\u044c\u043d\u0438\u0445 \u0432\u0443\u043b\u0438\u0446\u044f\u0445, \u0430 \u0432\u0443\u043b. \u0404\u0440\u0435\u0432\u0430\u043d\u0441\u044c\u043a\u0430, \u0437\u0433\u0456\u0434\u043d\u043e \u0434\u0430\u043d\u0438\u0445, \u043d\u0430\u0434\u0430\u043d\u0438\u0445 \u041a\u041a \u00ab\u041a\u0438\u0457\u0432\u0430\u0432\u0442\u043e\u0434\u043e\u0440\u00bb, \u043c\u0430\u0454 \u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0456\u044e \u043c\u0430\u0433\u0456\u0441\u0442\u0440\u0430\u043b\u044c\u043d\u043e\u0457 \u0432\u0443\u043b\u0438\u0446\u0456 \u0440\u0430\u0439\u043e\u043d\u043d\u043e\u0433\u043e \u0437\u043d\u0430\u0447\u0435\u043d\u043d\u044f.\r\n\u0417 \u043f\u043e\u0432\u0430\u0433\u043e\u044e, \u0437\u0430\u0441\u0442\u0443\u043f\u043d\u0438\u043a \u0434\u0438\u0440\u0435\u043a\u0442\u043e\u0440\u0430 \u041c. \u0406. \u041e\u043d\u0438\u0449\u0435\u043d\u043a\u043e\u201d.\r\n\r\n\u0425\u043e\u0447\u0443 \u0443\u0442\u043e\u0447\u043d\u0438\u0442\u044c, \u0447\u0442\u043e \u0441\u043e\u0433\u043b\u0430\u0441\u043d\u043e \u0442\u0430\u0431\u043b\u0438\u0446\u0435 1.2 \u0414\u0411\u041d \u0412.2.3-5-2001 \u00ab\u0434\u043e \u043c\u0430\u0433\u0456\u0441\u0442\u0440\u0430\u043b\u044c\u043d\u0438\u0445 \u0432\u0443\u043b\u0438\u0446\u044c \u0437\u0430\u0433\u0430\u043b\u044c\u043d\u043e\u043c\u0456\u0441\u044c\u043a\u043e\u0433\u043e \u0437\u043d\u0430\u0447\u0435\u043d\u043d\u044f \u0432\u0456\u0434\u043d\u043e\u0441\u044f\u0442\u044c\u0441\u044f \u0432\u0443\u043b\u0438\u0446\u0456 \u0437 \u0431\u0435\u0437\u043f\u0435\u0440\u0435\u0440\u0432\u043d\u0438\u043c \u0440\u0443\u0445\u043e\u043c 100 \u043a\u043c\/\u0433\u043e\u0434. \u0442\u0430 \u0456\u043d\u0442\u0435\u043d\u0441\u0438\u0432\u043d\u0456\u0441\u0442\u044e \u0440\u0443\u0445\u0443 1200 \u043f\u0440\u0438\u0432. \u043e\u0434.\/\u0433\u043e\u0434 \u043d\u0430 \u0441\u043c\u0443\u0433\u0443, \u0440\u0435\u0433\u0443\u043b\u044c\u043e\u0432\u0430\u043d\u0438\u043c \u0440\u0443\u0445\u043e\u043c 90 \u043a\u043c\/\u0433\u043e\u0434 \u0442\u0430 \u0456\u043d\u0442\u0435\u043d\u0441\u0438\u0432\u043d\u0456\u0441\u0442\u044e \u0440\u0443\u0445\u0443 700 \u043f\u0440\u0438\u0432. \u043e\u0434.\/\u0433\u043e\u0434 \u043d\u0430 \u0441\u043c\u0443\u0433\u0443\u00bb. \r\n\r\n\u0422\u0430\u043a\u0436\u0435 \u0432 \u0434\u0430\u043d\u043d\u043e\u0439 \u0442\u0430\u0431\u043b\u0438\u0446\u0435 \u0443\u043a\u0430\u0437\u0430\u043d\u043e, \u0447\u0442\u043e \u043c\u0430\u0433\u0438\u0441\u0442\u0440\u0430\u043b\u044c\u043d\u043e\u0439 \u0441\u0447\u0438\u0442\u0430\u0435\u0442\u0441\u044f \u0443\u043b\u0438\u0446\u0430, \u0448\u0438\u0440\u0438\u043d\u0430 \u043e\u0434\u043d\u043e\u0439 \u043f\u043e\u043b\u043e\u0441\u044b \u043a\u043e\u0442\u043e\u0440\u043e\u0439 3,75 \u043c\u0435\u0442\u0440\u043e\u0432 \u0438 \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u043e \u043f\u043e\u043b\u043e\u0441 \u0434\u043e\u043b\u0436\u043d\u043e \u0431\u044b\u0442\u044c 4-6. \u0410 \u043d\u0430 \u0415\u0440\u0435\u0432\u0430\u043d\u0441\u043a\u043e\u0439 \u0443\u043b\u0438\u0446\u0435 \u043f\u043e\u043b\u043e\u0441\u0430 \u043e\u0434\u043d\u0430, \u0442\u0430\u043c \u043e\u0434\u043d\u043e\u0441\u0442\u043e\u0440\u043e\u043d\u043d\u0435\u0435 \u0434\u0432\u0438\u0436\u0435\u043d\u0438\u0435. \u041f\u043b\u044e\u0441 \u0440\u044f\u0434\u043e\u043c \u0441 \u0434\u0430\u043d\u043d\u044b\u043c \u043f\u0435\u0440\u0435\u0445\u043e\u0434\u043e\u043c  \u0448\u043a\u043e\u043b\u0430.\r\n\r\n\u0420\u0443\u043a\u043e\u0432\