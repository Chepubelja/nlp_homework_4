u0415\u041c\u041d\u0423 \u0412\u0406\u0414\u041f\u0418\u0421\u041a\u0423. \u041e\u0441\u043e\u0431\u043b\u0438\u0432\u043e \u043e\u0431\u0443\u0440\u044e\u0454 \u041e\u0411\u041c\u0410\u041d \u0442\u0430 \u0426\u0418\u041d\u0406\u0417\u041c \u0443 \u0432\u0456\u0434\u043f\u0438\u0441\u0446\u0456 \u043d\u0430 \u043e\u0441\u0442\u0430\u043d\u043d\u0454 \u0437\u0432\u0435\u0440\u043d\u0435\u043d\u043d\u044f. \u0428-3387 \u0432\u0456\u0434 13.06.2014. \u0429\u0435 \u0439 \u0434\u043e\u0434\u0430\u043b\u0438 3 \u043b\u0438\u0441\u0442\u0438 \u201d\u0432\u043e\u0434\u0438\u0447\u043a\u0438\u201d \u043b\u0438\u0441\u0442\u0430 \u041c\u0456\u043d\u0440\u0435\u0433\u0456\u043e\u043d\u0431\u0443\u0434\u0443 (\u043d\u0435 \u0441\u043f\u0440\u043e\u043c\u043e\u0433\u043b\u0438\u0441\u044f \u043d\u0430\u0432\u0456\u0442\u044c \u0432\u0456\u0440\u043d\u043e \u0432\u043a\u0430\u0437\u0430\u0442\u0438 \u0440\u0435\u043a\u0432\u0456\u0437\u0438\u0442\u0438 \u043b\u0438\u0441\u0442\u0430 \u0456 \u043d\u0435 \u043d\u0430\u0434\u0430\u043d\u043e \u0441\u0430\u043c \u043b\u0438\u0441\u0442, \u0430 \u043b\u0438\u0448\u0435 \u0456\u043d\u0442\u0435\u0440\u043d\u0435\u0442- \u043a\u043e\u043f\u0456\u044f). \u041d\u0430\u0433\u043e\u043b\u043e\u0448\u0443\u044e - \u043b\u0438\u0441\u0442\u0438 \u041c\u0456\u043d\u0440\u0435\u0433\u0456\u043e\u043d\u0431\u0443\u0434\u0443 \u041d\u0415 \u0404 \u041d\u041e\u0420\u041c\u0410\u0422\u0418\u0412\u041d\u041e-\u041f\u0420\u0410\u0412\u041e\u0412\u0418\u041c\u0418 \u0410\u041a\u0422\u0410\u041c\u0418.  \r\n\u0406 \u0437\u043d\u043e\u0432\u0443 \u043a\u0435\u0440\u0456\u0432\u043d\u0438\u0446\u0442\u0432\u043e \u00ab\u041a\u043e\u043d\u0442\u0430\u043a\u0442\u043d\u043e\u0433\u043e \u0446\u0435\u043d\u0442\u0440\u0443 \u043c\u0456\u0441\u0442\u0430 \u041a\u0438\u0454\u0432\u0430\u00bb (\u0421\u0442\u0435\u0444\u0430\u043d\u043e\u0432\u0438\u0447 \u0404.\u0410.) \u0432 \u041f\u041e\u0420\u0423\u0428\u0415\u041d\u041d\u042f \u0441\u0442. 7 \u0417\u0423 \u201d\u041f\u0440\u043e \u0437\u0432\u0435\u0440\u043d\u0435\u043d\u043d\u044f \u0433\u0440\u043e\u043c\u0430\u0434\u044f\u043d\u201d \u201d....\u0417\u0430\u0431\u043e\u0440\u043e\u043d\u044f\u0454\u0442\u044c\u0441\u044f \u043d\u0430\u043f\u0440\u0430\u0432\u043b\u044f\u0442\u0438 \u0441\u043a\u0430\u0440\u0433\u0438 \u0433\u0440\u043e\u043c\u0430\u0434\u044f\u043d \u0434\u043b\u044f \u0440\u043e\u0437\u0433\u043b\u044f\u0434\u0443 \u0442\u0438\u043c \u043e\u0440\u0433\u0430\u043d\u0430\u043c \u0430\u0431\u043e \u043f\u043e\u0441\u0430\u0434\u043e\u0432\u0438\u043c \u043e\u0441\u043e\u0431\u0430\u043c, \u0434\u0456\u0457 \u0447\u0438 \u0440\u0456\u0448\u0435\u043d\u043d\u044f \u044f\u043a\u0438\u0445 \u043e\u0441\u043a\u0430\u0440\u0436\u0443\u044e\u0442\u044c\u0441\u044f\u201d \u043c\u043e\u0457 \u0441\u043a\u0430\u0440\u0433\u0438 \u0431\u0443\u043b\u043e \u043f\u0435\u0440\u0435\u043d\u0430\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u043e \u0441\u0430\u043c\u0435 \u041a\u041f \u201d\u0413\u0440\u0443\u0448\u043a\u0456\u0432\u0441\u044c\u043a\u0435\u201d! \u0410\u0432\u0442\u043e\u0440\u043e\u043c \u0432\u0456\u0434\u043f\u0438\u0441\u043a\u0438 \u0437\u043d\u043e\u0432\u0443 \u0454 \u0441\u0430\u043c\u0435 \u0434\u0438\u0440\u0435\u043a\u0442\u043e\u0440 \u041a\u041f \u201d\u0413\u0440\u0443\u0448\u043a\u0456\u0432\u0441\u044c\u043a\u0435\u201d \u0413\u043e\u0440\u043e\u0445 \u041c., \u0430 \u043f\u0430\u043d \u0432.\u043e. \u0433\u043e\u043b\u043e\u0432\u0438 \u0421\u0420\u0414\u0410 \u041b\u0430\u043f\u0430\u043d\u044c \u0410. \u043b\u0438\u0448\u0435 \u043f\u0456\u0434\u043c\u0430\u0445\u043d\u0443\u0432 \u0446\u044e \u0432\u0456\u0434\u043f\u0438\u0441\u043a\u0443. \r\n\u0429\u043e \u0441\u0442\u043e\u0441\u0443\u0454\u0442\u044c\u0441\u044f \u0441\u0443\u0442\u0456:\r\n1) \u041f\u043e\u0441\u0438\u043b\u0430\u043d\u043d\u044f \u043d\u0430 \u043b\u0438\u0441\u0442 \u041c\u0456\u043d\u0440\u0435\u0433\u0456\u043e\u043d\u0431\u0443\u0434\u0443 - \u0404 \u0411\u0415\u0417\u041f\u0406\u0414\u0421\u0422\u0410\u0412\u041d\u0418\u041c - \u0446\u0435\u0439 \u043b\u0438\u0441\u0442 \u043d\u0435 \u0454 \u041d\u041f\u0410!\r\n2) \u0423 \u0441\u043a\u0430\u0440\u0437\u0456 \u2116 \u0428-3012\/1 \u044f \u043d\u0430\u0432\u0456\u0432 \u0434\u043e\u0441\u0442\u0430\u0442\u043d\u044c\u043e \u043f\u043e\u0441\u0438\u043b\u0430\u043d\u044c, \u0446\u0435 \u0456 \u041f\u043e\u0441\u0442\u0430\u043d\u043e\u0432\u0430 \u041a\u041c\u0423 \u2116 869 \u0432\u0456\u0434 01.06.2011 \u0440., \u0456 \u0447.3 \u0441\u0442.28 \u0417\u0423 \u201d\u041f\u0440\u043e \u043c\u0435\u0442\u0440\u043e\u043b\u043e\u0433\u0456\u044e \u0442\u0430 \u043c\u0435\u0442\u0440\u043e\u043b\u043e\u0433\u0456\u0447\u043d\u0443 \u0434\u0456\u044f\u043b\u044c\u043d\u0456\u0441\u0442\u044c\u201d, \u044f\u043a\u0456 \u0437\u043e\u0431\u043e\u0432\u2019\u044f\u0437\u0443\u044e\u0442\u044c \u0412\u0438\u043a\u043e\u043d\u0430\u0432\u0446\u044f \u0432\u0438\u043a\u043e\u043d\u0430\u0442\u0438 \u043f\u043e\u0432\u0456\u0440\u043a\u0443 \u043b\u0456\u0447\u0438\u043b\u044c\u043d\u0438\u043a\u0456\u0432.\r\n3) \u0422\u043e\u0439 \u0444\u0430\u043a\u0442, \u0449\u043e \u041a\u041f \u201d\u0413\u0440\u0443\u0448\u043a\u0456\u0432\u0441\u044c\u043a\u0435\u201d \u044f\u043a \u0412\u0438\u043a\u043e\u043d\u0430\u0432\u0435\u0446\u044c \u0442\u0430 \u041a\u041f \u201d\u0414\u0438\u0440\u0435\u043a\u0446\u0456\u044f\u201d \u044f\u043a \u0431\u0430\u043b\u0430\u043d\u0441\u043e\u0443\u0442\u0440\u0438\u043c\u0443\u0432\u0430\u0447 \u0431\u0443\u0434\u0438\u043d\u043a\u0443 \u043d\u0435 \u0432\u043a\u043b\u044e\u0447\u0438\u043b\u0438 \u0456 \u043d\u0435 \u043f\u043e\u0434\u0430\u043b\u0438 \u043d\u0430 \u0437\u0430\u0442\u0432\u0435\u0440\u0434\u0436\u0435\u043d\u043d\u044f \u041e\u041c\u0421 \u0446\u0456 \u0432\u0438\u0442\u0440\u0430\u0442\u0438 \u0432 \u0441\u0442\u0440\u0443\u043a\u0442\u0443\u0440\u0443 \u0442\u0430\u0440\u0438\u0444\u0443, \u041d\u0415 \u0417\u041d\u0406\u041c\u0