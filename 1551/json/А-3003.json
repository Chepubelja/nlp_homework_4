u0437 \u0441\u043c\u0456\u0442\u0442\u0454\u043f\u0440\u043e\u0432\u043e\u0434\u043e\u043c. \u0426\u0435 \u0441\u043f\u0440\u0438\u0447\u0438\u043d\u044f\u0454 \u043f\u043e\u044f\u0432\u0443 \u0456 \u0440\u043e\u0437\u043f\u043e\u0432\u0441\u044e\u0434\u0436\u0435\u043d\u043d\u044f \u043f\u043e \u0432\u0441\u044c\u043e\u043c\u0443 \u043f\u0456\u0434\u2019\u0457\u0437\u0434\u0443 \u043d\u0435\u0441\u0442\u0435\u0440\u043f\u043d\u043e\u0433\u043e \u0437\u0430\u043f\u0430\u0445\u0443 \u0433\u043e\u0440\u0456\u043b\u043e\u0433\u043e. \u041a\u0440\u0456\u043c \u0442\u043e\u0433\u043e, \u0437\u0437\u043e\u0432\u043d\u0456 \u043f\u043e \u0442\u0440\u0443\u0431\u0456 \u0441\u0442\u0456\u043a\u0430\u0454 \u0441\u043c\u0435\u0440\u0434\u044e\u0447\u0438\u0439 \u043a\u043e\u043d\u0434\u0435\u043d\u0441\u0430\u0442, \u0449\u043e \u0441\u0432\u0456\u0434\u0447\u0438\u0442\u044c \u043f\u0440\u043e \u0432\u0456\u0434\u0441\u0443\u0442\u043d\u0456\u0441\u0442\u044c \u0433\u0435\u0440\u043c\u0435\u0442\u0438\u0447\u043d\u043e\u0441\u0442\u0456 \u0442\u0430 \u0442\u0435, \u0449\u043e \u0434\u0438\u043c\u043e\u0445\u0456\u0434 \u043d\u0435 \u0447\u0438\u0441\u0442\u0438\u0442\u044c\u0441\u044f. \u042f\u043a \u0432\u0434\u0430\u043b\u043e\u0441\u044f \u0437\u2019\u044f\u0441\u0443\u0432\u0430\u0442\u0438 \u0443 \u0413\u043e\u043b\u043e\u0432\u043d\u043e\u043c\u0443 \u0443\u043f\u0440\u0430\u0432\u043b\u0456\u043d\u043d\u0456 \u0414\u0435\u0440\u0436\u0441\u0430\u043d\u0435\u043f\u0456\u0434\u0441\u043b\u0443\u0436\u0431\u0438 \u0443 \u043c. \u041a\u0438\u0454\u0432\u0456 \u0442\u0430 \u0443 \u041f\u043e\u0434\u0456\u043b\u044c\u0441\u044c\u043a\u0456\u0439 \u0440\u0430\u0439\u043e\u043d\u043d\u0456\u0439 \u0432 \u043c. \u041a\u0438\u0454\u0432\u0456 \u0434\u0435\u0440\u0436\u0430\u0432\u043d\u0456\u0439 \u0430\u0434\u043c\u0456\u043d\u0456\u0441\u0442\u0440\u0430\u0446\u0456\u0457, \u0434\u0430\u043d\u0438\u0439 \u0437\u0430\u043a\u043b\u0430\u0434 \u043d\u0435 \u043c\u0430\u0454 \u0436\u043e\u0434\u043d\u0438\u0445 \u0434\u043e\u0437\u0432\u0456\u043b\u044c\u043d\u0438\u0445 \u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442\u0456\u0432. \u0422\u0430\u043a\u043e\u0436 \u0431\u0443\u043b\u043e \u043d\u0430\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0435 \u0437\u0432\u0435\u0440\u043d\u0435\u043d\u043d\u044f \u0434\u043e \u043a\u0435\u0440\u0456\u0432\u043d\u0438\u0446\u0442\u0432\u0430 \u0414\u0435\u043f\u0430\u0440\u0442\u0430\u043c\u0435\u043d\u0442\u0443 \u0437\u0430\u043f\u043e\u0431\u0456\u0433\u0430\u043d\u043d\u044f \u043d\u0430\u0434\u0437\u0432\u0438\u0447\u0430\u0439\u043d\u0438\u0445 \u0441\u0438\u0442\u0443\u0430\u0446\u0456\u0439 \u0442\u0430 \u0434\u0435\u0440\u0436\u0430\u0432\u043d\u043e\u0433\u043e \u043d\u0430\u0433\u043b\u044f\u0434\u0443 (\u043a\u043e\u043d\u0442\u0440\u043e\u043b\u044e) \u0414\u0421\u041d\u0421 \u0423\u043a\u0440\u0430\u0457\u043d\u0438, \u0430\u043b\u0435 \u0432\u0456\u0434 \u043d\u0438\u0445 \u0432\u0456\u0434\u043f\u043e\u0432\u0456\u0434\u044c \u0449\u0435 \u043d\u0435 \u043d\u0430\u0434\u0456\u0439\u0448\u043b\u0430.\r\n\u0415\u043a\u0441\u043f\u043b\u0443\u0430\u0442\u0443\u044e\u0447\u0430 \u043e\u0440\u0433\u0430\u043d\u0456\u0437\u0430\u0446\u0456\u044f, \u0422\u041e\u0412 \u00ab\u0415\u043a\u043e\u043c\u0456\u0441\u0442\u043e\u00bb, \u0441\u0442\u0432\u0435\u0440\u0434\u0436\u0443\u0454, \u0449\u043e \u0434\u043e\u0437\u0432\u043e\u043b\u0456\u0432 \u0430\u043d\u0456 \u043d\u0430 \u043f\u0440\u043e\u0432\u0435\u0434\u0435\u043d\u043d\u044f \u0434\u0438\u043c\u043e\u0445\u043e\u0434\u0443 \u0432 \u0431\u0443\u0434\u0438\u043d\u043a\u0443, \u0430\u043d\u0456 \u043d\u0430 \u0440\u043e\u0437\u043c\u0456\u0449\u0435\u043d\u043d\u044f \u043b\u0456\u0442\u043d\u0456\u0445 \u043c\u0430\u0439\u0434\u0430\u043d\u0447\u0438\u043a\u0456\u0432 \u043d\u0430 \u043f\u0440\u0438\u0431\u0443\u0434\u0438\u043d\u043a\u043e\u0432\u0456\u0439 \u0442\u0435\u0440\u0438\u0442\u043e\u0440\u0456\u0457 \u043d\u0435 \u0434\u0430\u0432\u0430\u043b\u0438. \u0412 \u0442\u043e\u0439 \u0436\u0435 \u0447\u0430\u0441 \u0443\u043c\u043e\u0432\u0438 \u043f\u0440\u043e\u0436\u0438\u0432\u0430\u043d\u043d\u044f \u043d\u0435\u0441\u0442\u0435\u0440\u043f\u043d\u0456 \u0442\u0430 \u043d\u0435 \u0432\u0456\u0434\u043f\u043e\u0432\u0456\u0434\u0430\u044e\u0442\u044c \u043d\u043e\u0440\u043c\u0430\u043c, \u043e\u0441\u043a\u0456\u043b\u044c\u043a\u0438  \u0447\u0435\u0440\u0435\u0437 \u0434\u0438\u043c\u043e\u0445\u0456\u0434 \u0432 \u043f\u0456\u0434\u2019\u0457\u0437\u0434\u0456 \u043f\u043e\u0441\u0442\u0456\u0439\u043d\u0438\u0439 \u0441\u043c\u043e\u0440\u0456\u0434, \u0430 \u043d\u0430\u0432\u043a\u043e\u043b\u043e \u0434\u0438\u043c\u043e\u0445\u043e\u0434\u0443 \u0431\u0440\u0443\u0434 \u0438 \u0436\u0438\u0440\u043d\u0456 \u043f\u0430\u0442\u044c\u043e\u043a\u0438. \u0421\u0430\u043c\u0430 \u0435\u043a\u0441\u043f\u043b\u0443\u0430\u0442\u0443\u044e\u0447\u0430 \u043e\u0440\u0433\u0430\u043d\u0456\u0437\u0430\u0446\u0456\u044f \u0432\u0436\u0438\u0432\u0430\u0442\u0438 \u0431\u0443\u0434\u044c-\u044f\u043a\u0456 \u0437\u0430\u0445\u043e\u0434\u0438 \u0432\u0456\u0434\u043c\u043e\u0432\u043b\u044f\u044e\u0442\u044c\u0441\u044f, \u043f\u043e\u0441\u0438\u043b\u0430\u044e\u0447\u0438\u0441\u044c \u043d\u0430 \u0442\u0435, \u0449\u043e \u043d\u0435 \u0454 \u0432\u043b\u0430\u0441\u043d\u0438\u043a\u0430\u043c\u0438 \u043e\u0440\u0435\u043d\u0434\u043e\u0432\u0430\u043d\u0438\u0445 \u0440\u0435\u0441\u0442\u043e\u0440\u0430\u043d\u043e\u043c \u043f\u0440\u0438\u043c\u0456\u0449\u0435\u043d\u044c.  \r\n\u0423 \u041f\u043e\u0434\u0456\u043b\u044c\u0441\u044c\u043a\u0456\u0439 \u0440\u0430\u0439\u043e\u043d\u043d\u0456\u0439 \u0432 \u043c. \u041a\u0438\u0454\u0432\u0456 \u0434\u0435\u0440\u0436\u0430\u0432\u043d\u0456\u0439 \u0430\u0434\u043c\u0456\u043d\u0456\u0441\u0442\u0440\u0430\u0446\u0456\u0457 \u043f\u043e\u0432\u0456\u0434\u043e\u043c\u0438\u043b\u0438, \u0449\u043e \u043a\u0435\u0440\u0456\u0432